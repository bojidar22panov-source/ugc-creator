{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/:path((?!api/).*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 60
        }
    }
}